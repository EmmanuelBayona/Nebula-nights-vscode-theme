    "name": "nebbula-nights",
    "author": "Emmanuel",
    "maintainers": ["Emmanuel <bayonaj368@gmail.com>"],
    "type": "dark",
    "colors": {
        "editor.background": "#171717",
        "editor.selectionBackground": "#4a044e7a",
        "editorLineNumber.foreground": "#737373",
        "editorLineNumber.activeForeground": "#A3A3A3",
        "editorCursor.foreground": "#A3A3A3",
        "editorGroupHeader.tabsBackground": "#171717",
        "editorGroupHeader.tabsBorder": "#1C1C1C",
        "editorIndentGuide.background1": "#1C1C1C",
        "editorWhitespace.foreground": "#1C1C1C",

        "tab.inactiveBackground": "#171717",
        "tab.activeBackground": "#171717",
        "tab.activeForeground": "#b1b1b1",
        "tab.inactiveForeground": "#737373",
        "tab.activeBorder": "#737373",

        "activityBar.background": "#171717",
        "activityBar.activeBackground": "#171717",
        "activityBarBadge.background": "#4a044e",
        "activityBarBadge.foreground": "#d946ef",
        "activityBar.activeBorder": "#A3A3A3",
        "activityBar.inactiveForeground": "#737373",
        "activityBar.foreground": "#A3A3A3",

        "sideBar.background": "#171717",
        "sideBar.border": "#1C1C1C",
        "sideBar.foreground": "#737373",
        "sideBarTitle.foreground": "#A3A3A3",
        "sideBarSectionHeader.background": "#171717",
        "sideBarSectionHeader.foreground": "#A3A3A3",

        "statusBar.background": "#171717",
        "statusBar.foreground": "#737373",
        "statusBar.noFolderBackground": "#171717",
        "statusBar.border": "#1C1C1C",

        "titleBar.activeBackground": "#171717",
        "titleBar.inactiveBackground": "#171717",
        "titleBar.activeForeground": "#A3A3A3",
        "titleBar.inactiveForeground": "#737373",

        "gitDecoration.modifiedResourceForeground": "#d946ef",

        "scrollbar.shadow": "#171717",
        "scrollbarSlider.background": "#1C1C1C",
        "scrollbarSlider.hoverBackground": "#1C1C1C",
        "scrollbarSlider.activeBackground": "#1C1C1C",

        "list.activeSelectionBackground": "#1C1C1C",
        "list.activeSelectionForeground": "#A3A3A3",
        "list.inactiveSelectionBackground": "#1C1C1C",
        "list.hoverBackground": "#1C1C1C",
        "list.focusAndSelectionOutline": "#737373",
        "list.warningForeground": "#f59e0b",
        "list.errorForeground": "#f43f5e",

        "tree.indentGuidesStroke": "#737373",
        "tree.inactiveIndentGuidesStroke": "#1C1C1C",

        "input.background": "#171717",
        "input.foreground": "#A3A3A3",
        "input.border": "#1C1C1C",

        "editorWidget.background": "#171717",
        "editorWidget.border": "#1C1C1C",
        "editorWidget.foreground": "#A3A3A3",

        "widget.shadow": "#171717",
        "widget.border": "#1C1C1C",

        "button.background": "#171717",
        "button.foreground": "#d946ef",
        "button.border": "#d846ef1a",

        "textLink.foreground": "#b1b1b1",
        "textLink.activeForeground": "#d846efad",

        "focusBorder": "#1C1C1C",

        "panel.border": "#1C1C1C",

        "progressBar.background": "#d946ef"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#5c547a"
            }
        },
        {
            "name": "Variables",
            "scope": ["variable", "string constant.other.placeholder"],
            "settings": {
                "foreground": "#EEFFFF"
            }
        },
        {
            "name": "Colors",
            "scope": ["constant.other.color"],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Invalid",
            "scope": ["invalid", "invalid.illegal"],
            "settings": {
                "foreground": "#f43f5e"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#C792EA"
            }
        },
        {
            "name": "Operator, Misc",
            "scope": [
                "keyword.control",
                "constant.other.color",
                "punctuation",
                "meta.tag",
                "punctuation.definition.tag",
                "punctuation.separator.inheritance.php",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html",
                "punctuation.section.embedded",
                "keyword.other.template",
                "keyword.other.substitution"
            ],
            "settings": {
                "foreground": "#89DDFF"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#ec4899"
            }
        },
        {
            "name": "Function, Special Method",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#82AAFF"
            }
        },
        {
            "name": "Block Level Variables",
            "scope": ["meta.block variable.other"],
            "settings": {
                "foreground": "#ec4899"
            }
        },
        {
            "name": "Other Variable, String Link",
            "scope": ["support.other.variable", "string.other.link"],
            "settings": {
                "foreground": "#ec4899"
            }
        },
        {
            "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "variable.parameter",
                "keyword.other.unit",
                "keyword.other"
            ],
            "settings": {
                "foreground": "#f97316"
            }
        },
        {
            "name": "String, Symbols, Inherited Class, Markup Heading",
            "scope": [
                "string",
                "constant.other.symbol",
                "constant.other.key",
                "entity.other.inherited-class",
                "markup.heading",
                "markup.inserted.git_gutter",
                "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
            ],
            "settings": {
                "foreground": "#34d399"
            }
        },
        {
            "name": "Class, Support",
            "scope": [
                "entity.name",
                "support.type",
                "support.class",
                "support.other.namespace.use.php",
                "meta.use.php",
                "support.other.namespace.php",
                "markup.changed.git_gutter",
                "support.type.sys-types"
            ],
            "settings": {
                "foreground": "#fbbf24"
            }
        },
        {
            "name": "Entity Types",
            "scope": ["support.type"],
            "settings": {
                "foreground": "#B2CCD6"
            }
        },
        {
            "name": "CSS Class and Support",
            "scope": [
                "source.css support.type.property-name",
                "source.sass support.type.property-name",
                "source.scss support.type.property-name",
                "source.less support.type.property-name",
                "source.stylus support.type.property-name",
                "source.postcss support.type.property-name"
            ],
            "settings": {
                "foreground": "#B2CCD6"
            }
        },
        {
            "name": "Sub-methods",
            "scope": [
                "entity.name.module.js",
                "variable.import.parameter.js",
                "variable.other.class.js"
            ],
            "settings": {
                "foreground": "#f43f5e"
            }
        },
        {
            "name": "Language methods",
            "scope": ["variable.language"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#f43f5e"
            }
        },
        {
            "name": "entity.name.method.js",
            "scope": ["entity.name.method.js"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#82AAFF"
            }
        },
        {
            "name": "meta.method.js",
            "scope": [
                "meta.class-method.js entity.name.function.js",
                "variable.function.constructor"
            ],
            "settings": {
                "foreground": "#82AAFF"
            }
        },
        {
            "name": "Attributes",
            "scope": ["entity.other.attribute-name"],
            "settings": {
                "foreground": "#C792EA"
            }
        },
        {
            "name": "HTML Attributes",
            "scope": [
                "text.html.basic entity.other.attribute-name.html",
                "text.html.basic entity.other.attribute-name"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#FFCB6B"
            }
        },
        {
            "name": "CSS Classes",
            "scope": ["entity.other.attribute-name.class"],
            "settings": {
                "foreground": "#FFCB6B"
            }
        },
        {
            "name": "CSS ID's",
            "scope": ["source.sass keyword.control"],
            "settings": {
                "foreground": "#82AAFF"
            }
        },
        {
            "name": "Inserted",
            "scope": ["markup.inserted"],
            "settings": {
                "foreground": "#34d399"
            }
        },
        {
            "name": "Deleted",
            "scope": ["markup.deleted"],
            "settings": {
                "foreground": "#f43f5e"
            }
        },
        {
            "name": "Changed",
            "scope": ["markup.changed"],
            "settings": {
                "foreground": "#C792EA"
            }
        },
        {
            "name": "Regular Expressions",
            "scope": ["string.regexp"],
            "settings": {
                "foreground": "#89DDFF"
            }
        },
        {
            "name": "Escape Characters",
            "scope": ["constant.character.escape"],
            "settings": {
                "foreground": "#89DDFF"
            }
        },
        {
            "name": "URL",
            "scope": ["*url*", "*link*", "*uri*"],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Decorators",
            "scope": [
                "tag.decorator.js entity.name.tag.js",
                "tag.decorator.js punctuation.definition.tag.js"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#82AAFF"
            }
        },
        {
            "name": "ES7 Bind Operator",
            "scope": [
                "source.js constant.other.object.key.js string.unquoted.label.js"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#f43f5e"
            }
        },
        {
            "name": "JSON Key - Level 0",
            "scope": [
                "source.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#C792EA"
            }
        },
        {
